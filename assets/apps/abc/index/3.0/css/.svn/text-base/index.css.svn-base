/*
===================
@explain: 系统后台框架结构
===================
*/
body, div, dl, dt, dd, ul, ul li, form, p, th, td {margin: 0;padding: 0;}
input, button{font-family: inherit;font-size: inherit;font-style: inherit;font-weight: inherit;}
cite, em, strong, th {font-style: inherit;font-weight: inherit;}
td, th, div {word-break:break-all;word-wrap:break-word;}
table {border-collapse: collapse;border-spacing:0;}
th {text-align: left;font-weight:100;}
ul, ul li{list-style: none;}
form {display:inline;}
img {border: 0;}
/*
===================
清除浮动
* cc 作用于父模型
===================
*/
.cc{
	zoom:1;
}
.cc:after{
	content:'\20';
	display:block;
	height:0;
	clear:both;
	visibility: hidden;
}
/*
===================
背景组合
===================
*/
.tab td,
.tabA_pre,
.tabA_next,
.tabA_pre_old,
.tabA_next_old,
.tabA li .del,
.menubar dt a,
.menuNext a,
.search button,
.refresh,
.full_screen,
.options{
	background: url(images/admin/layout/bg.png);
}
.logo{
	background: url(./images/logo.png) center center no-repeat;
}
/*
===================
结构
===================
*/
html,
body {
	overflow: hidden;
	width: 100%;
	height: 100%;
	font-family: Arial;
	color: #333;
	font-size: 12px;
}
iframe {
	height: 100%;
	width: 100%;
}
a {
	color: #333;
	text-decoration: none;
}
.wrap {
	min-width: 880px;
}
/*
===================
头部
===================
*/
.head {
	height: 46px;
	background:#4192e8 url(./images/headerbg.png) repeat-x;
	border-bottom:1px solid #1257a9
}
.head th {
	width: 180px;
}
.logo {
	display: block;
	width: 170px;
	height: 46px;
	overflow: hidden;
	text-indent: -2000em;
}
.nav {
	float: left;
	padding:0 0 0 50px;
}
.nav ul {
	float: left;
}
.nav li {
	float: left;
	position: relative;
	z-index: 10;
}
.nav li a {
	float: left;
	display: block;
	height: 44px;
	line-height:44px;
	font-size: 14px;
	padding:0 15px;
	text-align: center;
	color:#fff;
	background: url(./images/sp1.png) 100% 0 no-repeat
}

.nav li a:hover,.nav li.current a {
	background: #377ac5;
	color: #fff;
}

.nav li span{ float: left;padding: 0 10px 0 0;}
.nav li a em {
	display: inline-block;
	width: 7px;
	height: 5px;
	background: url(./images/arrow-down.png?003) no-repeat;
	float: right;
	margin: 19px 0 0;
	cursor: pointer;
}

.nav .t-subMenu{ 
	position: absolute; 
	top: 44px; 
	font-size: 12px;
	z-index: 10000; 
	left: 0;
	min-width: 103px;
}

.nav .t-subMenu p{ 
	cursor: pointer; 
	height: 29px; 
	line-height: 29px; 
	text-align: center; 
}
.nav .t-subMenu p a{ 
	float: none; 
	height: 29px;
	line-height: 29px;
	padding: 0;
	background: #4a8dd8;
	color:#fff;  
}
.nav .t-subMenu p a:hover{ 
	background: #1f65b2;
}


.login_info {
	float: right;
	font-size: 12px;
	color: #033c81;
	line-height: 30px;
	padding:10px 10px 0 0;
	white-space: nowrap;
	text-align: right;
	font-weight: bold;
}

.login_info  i {	
	display: inline-block;
	background: url(images/admin/layout/icon.png?0036) 0 -21px no-repeat; 
	width: 12px; 
	height: 12px; 
	margin: 0 5px 0 0
}

.login_info a {
	color: #033c81;
	margin:0 5px;
}

.login_info a:hover {
	color: #fff;
}

.subTitle{
	font-weight: bold;  
	font-size: 14px; 
	color: #458; 
	padding: 0 0 0 18px; 
	margin: 0; 
	line-height: 30px;
	background: #dfe9f6 url(./images/bbg.png) bottom repeat-x;
}

/*
===================
面包屑
===================
*/
#breadCrumb{
	font-size:12px;
	padding:10px 20px;
	width: 100%;
	color: #fff;
	background: #c6c6c6;
}
/*
===================
左侧菜单
===================
*/
.content th {
	background: #dfe9f6;
	vertical-align: top;
	font-weight: 100;
}
.menubar {
	padding:0 0 10px;
	overflow: hidden;
	font-weight: 100;
}
.menubar dl {
	padding: 0;
	margin: 0;
}
.menubar dt {
	font-size: 12px;
}
.menubar dt a{background: url(./images/bbg.png) bottom repeat-x;}
.menubar dt.current a{background: none;}
.menubar dt a {
	display: block;
	line-height: 40px;
	height: 40px;
	overflow: hidden;
	padding: 0 10px 0 30px;
	color: #458;
	outline: none;
}
.menubar dt a em{
	float: left; 
	display: inline-block;
	background: url(./images/arrow-l1.png)  no-repeat;
	width: 9px;
	height: 9px; 
	margin: 15px 10px 0 0;
}

.menubar dt.current a em{
	background: url(./images/arrow-l2.png)  no-repeat;
}
.menubar dd{background: url(./images/bbg.png) bottom repeat-x;}
.menubar ul li {
	padding: 0;
	font-size: 12px;
}
.menubar ul li a {
	display: block;
	line-height:30px;
	height:30px;
	overflow: hidden;
	padding: 0 5px 0 50px;
	outline: none;
	color: #68c;
}
.menubar ul li a.current{
	background-color: #7cb3f6;
	border-top: 1px solid #5a91d3;
	border-bottom: 1px solid #5a91d3;
	color: #fff
}
.menubar ul li a.current:hover{
	background-color: #7cb3f6;
}
.menubar ul li a:hover{
	background: #cce2ff;
}
/*
===================
左侧菜滚动切换
===================
*/
.menuNext {
	height: 21px;
	position: absolute;
	bottom: 0;
	left: 0;
	width: 140px;
}
.menuNext a {
	float: left;
	width: 70px;
	height: 21px;
	text-indent: -2000em;
	overflow: hidden;
}
.menuNext a.pre{
	background-position:0 -480px;
}
.menuNext a.next{
	background-position:-70px -480px;
}
.menuNext a.pre:hover {
	background-position: 0 -510px;
}
.menuNext a.next:hover {
	background-position: -70px -510px;
}
.menuNext a.pre_old,
.menuNext a.pre_old:hover {
	background-position: 0 -541px;
}
.menuNext a.next_old,
.menuNext a.next_old:hover {
	background-position: -70px -541px;
}
/*
===================
选项区域
===================
*/
.tab th,
.tab td {

}
.tab th {
	background: #e8e8e8;
	font-weight: 100;
	height: 30px;
}
.tab td {
	vertical-align: top;
	padding: 0;
}
/*
===================
搜索
===================
*/
.search {
	height:22px;
	width: 128px;
	overflow: hidden;
	padding: 0;
	border:1px solid #e5e5e5;
	background:#fff;
	position:relative;
}
.search input {
	left:0;
	top:2px;
	border: 0 none;
	padding:0 5px;
	font: 12px Arial;
	vertical-align: middle;
	width: 105px;
	background:none;
	position:absolute;
}
.search input:focus{
	outline:0 none;
}
.search button {
	position:absolute;
	top:3px;
	right:5px;
	border: 0 none;
	height: 16px;
	cursor: pointer;
	width: 22px;
	text-indent:-2000em;
	overflow:hidden;
	background-position:-145px -479px;
}
.gray {
	color: #999;
}
/*
===================
选项卡
===================
*/
.tab td{
	background-position:0 -300px;
	background-color:#f7f7f7;
}
.tabA ul {
	line-height: 30px;
	height: 30px;
	overflow: hidden;
	border-right: 1px solid #dedede;
}
.tabA li {
	float: left;
	font-size: 12px;
}
.tabA li span {
	display: block;
	line-height: 30px;
	height: 30px;
	padding: 0 10px 0 15px;
	float: left;
	white-space: nowrap;
	cursor: pointer;
}
.tabA li span:hover {
	background-position: 0 -360px;
}
.tabA li a {
	outline: none;
	float: left;
	font-style: normal;
	color: #999;
	padding-right: 8px;
}
.tabA li a:hover {
	color: #000;
}
.tabA li:focus{
}
.tabA li .del {
	width: 7px;
	height: 7px;
	overflow: hidden;
	display: block;
	text-indent: -2000em;
	background-position: -20px -430px;
	margin: 11px 0 0 0;
	position: relative;
	padding: 0;
}
.tabA li .del:hover {
	background-position: -27px -430px;
}
.tabA li.current .del {
	background-position: -34px -430px;
}
.tabA li.current .del:hover {
	background-position: -41px -430px;
}

.tabA li.current span {
	background:#c6c6c6;
}
.tabA li.current a,
.tabA li.current a:hover{
	font-weight: 100;
	color: #fff;
}
.tabA .fr {
	margin-top: 10px;
}
.tabA_pre,
.tabA_next,
.tabA_pre_old,
.tabA_next_old {
	width: 5px;
	height: 9px;
	display: block;
	overflow: hidden;
	text-indent: -2000em;
	margin: 10px 8px 0;
	*margin: 10px 5px 0 8px;
}
.tabA_pre {
	background-position: 0 -430px;
	float: left;
}
.tabA_next {
	background-position: -4px -430px;
	float: right;
}
.tabA_pre_old {
	background-position: -9px -430px;
	float: left;
}
.tabA_next_old {
	background-position: -13px -430px;
	float: right;
}
/*
===================
内容区域管理
===================
*/
.options {
	position: absolute;
	width:54px;
	height:22px;
	right: 25px;
	top: 85px;
	background-position:-180px -500px;
}
.options a{
	float:left;
	height:22px;
	line-height:22px;
	width:27px;
}
.refresh,
.full_screen{
	text-indent:-2000em;
	overflow:hidden;
}
.refresh{
	background-position:-243px -477px;
}
.full_screen{
	background-position:-173px -475px;
}
/*
===================
关闭js提醒
===================
*/
.noscript {
	background: #fffbbb;
	line-height: 20px;
	height: 20px;
	text-align: center;
	font-size: 12px;
	position: absolute;
	width: 100%;
	left: 0;
	top: 0;
	padding:0;
	margin:0;
}
/*
===================
加载状态
===================
*/
.loading{
	border:1px solid #e4e4e4;
	background:#fffde3;
	line-height:100%;
	padding: 6px 10px;
	position:absolute;
	border-radius: 2px;
	-webkit-border-radius: 2px;
	box-shadow: 0 0 5px #e4e4e4;
	-webkit-box-shadow: 0 0 5px #e4e4e4;
	color:#222;
	font-weight:700;
	font-size:12px;
	margin-left:10px;
	top:130px;
}

/**
 * 全屏样式
 */ 
.fullScreen .content th{
	width:0;
	display:none;
}
.fullScreen .head,.fullScreen .tab{
	height:0;
	display:none;
}
.fullScreen #default{
	  *left:0;
	  *top:-90px;
}
.fullScreen div.options{
    top:0;
}
