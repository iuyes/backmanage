/*
*客户360样式
*/
.custom-add-link,.custom-remove-link,.custom-fold-link,.custom-red-star,.custom-green-star,.choice-category-wrap p a{
	background:url(images/admin/detail360icon.png?002) no-repeat;
}
.custom-add-link,.custom-remove-link{ display: inline-block; text-indent: -9999em; cursor: pointer; width: 12px; height: 12px}
.custom-remove-link{ background-position:0 -14px }
.custom-red-star,.custom-green-star{ display: inline-block; width: 14px; height: 16px; background-position: 0 -75px}
.custom-green-star{ background-position: 0 -91px}
.custom-choice-btn{ width: 77px; height: 24px; display: inline-block; color: #222;}
.custom-choice-btn:hover{ text-decoration: none;}
.custom-choice-btn span{ padding: 5px 0 0 20px}
.show-choice-btn{ background: url(images/admin/btn3602.png) no-repeat}
.hide-choice-btn{ background: url(images/admin/btn3601.png) no-repeat}
.choice-wrap{ border:1px dotted #bbb; background-color: #fff; padding: 5px; line-height: 20px; display: inline-block; -webkit-border-radius:3px;-moz-border-radius:3px;border-radius:3px; }
.choice-wrap label{ padding: 0 10px 0 0; color: #bbb}

.choice-category{ background-color: #fff;  border:1px solid #e5e3e3; margin: 5px 0 0;-webkit-box-shadow:0 3px 4px rgba(0,0,0,.25);box-shadow:0 3px 4px rgba(0,0,0,.25);-moz-box-shadow:0 3px 4px rgba(0,0,0,.25); }
.choice-category .c-title{ color: #000; display: inline-block; line-height: 24px; text-align: right; width: 100%; }
.choice-category-wrap p{ border-top: 1px solid #cfe0f5; text-align: center;}
.choice-category-wrap p a{display: inline-block; width: 87px; height: 18px; background-position: 0 -34px; text-indent: -9999em}
.choice-category-wrap p a:hover{ background-position: 0 -56px}


.custom-contact-w{ position: absolute; height: 20px; top:-18px; width: 100%; text-align: center;}
.custom-contact-w a{ display: inline-block;background: url(images/admin/contact2.png) no-repeat; width: 87px; height: 18px; text-indent: -9999em}
.custom-contact-wi a{background: url(images/admin/contact1.png) no-repeat; }
/*搜索页面*/
.searchWrap{ width: 603px; height: 53px; background:url(images/admin/searchbg.png?002) no-repeat }
.searchWrap-con{ padding:13px 15px  10px}
.search-btn{ border: none; background: url(images/admin/searchbtn.png) no-repeat; width: 77px; height: 28px; cursor: pointer;}
.seacrch-noresult{ padding:40px 0; background: url(images/admin/noresultbg.png) 270px 40px no-repeat; height: 65px}
.seacrch-noresult p{ padding: 0 0 0 370px; font-size: 16px; color: #000; line-height: 65px; font-weight: bold;}

/*联系小记右侧样式*/
.custom-contact-quicksupport{position: fixed;_position: absolute; right: 0; top: 50px; width:900px; z-index: 100}
.custom-contact-quicksupport .quicksupport-header{position: absolute; width: 26px; height: 96px; background:url(images/admin/contactbg.png) 0 0 no-repeat; text-indent: -9999em; margin-left: -26px; cursor: pointer;}
.quicksupport-contact-fold{ width: 0; margin-left: -900px}
.custom-contact-quicksupport .quicksupport-body{}
.quicksupport-contact-fold .quicksupport-header{background-position: -26px 0}
.custom-contact-wrap{z-index: 10; background: #fff; border: 1px solid #e4e4e4}
.custom-contact-wrap p.noteListCon{line-height:20px;white-space:nowrap;overflow:hidden;width:500px;height:20px;}

.customer-category-wrap{ position: relative; display: block;}
.customer-category-con{width:400px;position: absolute; left:0;top:30px;background-color: #fff;  border:1px solid #e5e3e3; margin: 5px 0 0;-webkit-box-shadow:0 3px 4px rgba(0,0,0,.25);box-shadow:0 3px 4px rgba(0,0,0,.25);-moz-box-shadow:0 3px 4px rgba(0,0,0,.25); z-index:10
}
.customer-category-con .customer-category-header{padding: 0 10px}
.customer-category-con .switch_list{ padding: 5px 10px}
.customer-category-con .pop_close{ position: absolute; right: 3px; top: 3px}

